{
  "name": "ai-assistant-pro",
  "private": true,
  "version": "1.0.0",
  "description": "Professional AI Assistant with multiple AI providers",
  "main": "electron/main.js",
  "homepage": "./",
  "author": "Roberto Torres",
  "type": "module",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "Chrome >= 70",
    "Firefox >= 65",
    "Safari >= 12",
    "Edge >= 79"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "vercel-build": "npm run build",
    "build-static": "npm run build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "deploy": "npm run build && vercel --prod",
    "clean": "rimraf dist node_modules/.vite",
    "clean:install": "rimraf node_modules package-lock.json && npm install",
    "type-check": "tsc --noEmit",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:dist": "npm run build && electron-builder --publish=never",
    "electron:pack": "npm run build && electron-builder --dir",
    "build:all": "npm run build && npm run electron:dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google/generative-ai": "^0.2.1",
    "@langchain/anthropic": "0.3.30",
    "@langchain/core": "0.3.78",
    "@langchain/google-genai": "0.2.18",
    "@langchain/openai": "0.6.14",
    "@stripe/stripe-js": "^2.1.0",
    "@supabase/supabase-js": "^2.38.0",
    "@tanstack/react-query": "^4.35.0",
    "axios": "^1.5.0",
    "canvas": "3.2.0",
    "cheerio": "1.1.2",
    "crypto-js": "^4.2.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.263.1",
    "mammoth": "1.11.0",
    "node-html-parser": "7.0.1",
    "openai": "^4.28.0",
    "pdf-parse": "1.1.1",
    "pdfjs-dist": "^4.0.379",
    "prismjs": "^1.30.0",
    "puppeteer": "24.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.15.0",
    "react-syntax-highlighter": "^15.6.6",
    "recharts": "^2.8.0",
    "remark-gfm": "^4.0.0",
    "sharp": "0.34.4",
    "uuid": "^9.0.0",
    "xlsx": "0.18.5",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@eslint/object-schema": "^2.1.6",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^8.2.2",
    "core-js": "^3.38.1",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^0.20.5",
    "wait-on": "^7.2.0",
    "workbox-window": "^7.1.0"
  },
  "build": {
    "appId": "com.aiassistantpro.app",
    "productName": "AI Assistant Pro",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
